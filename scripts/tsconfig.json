{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2020",
		"lib": ["ES2020"],
		"sourceMap": true,
		"rootDir": "..",
		"strict": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedParameters": true,
		"noImplicitOverride": true,
		"skipLibCheck": true,
		"noEmit": true
	},
	"include": ["./generate-package-json.ts"]
}
